      },
      {
         "label": "Build release",
         "type": "shell",
         "command": "nmake",
         "args": [
            "-NOLOGO"
         ],
         "problemMatcher": {
            "base": "$msCompile",
            "fileLocation": "absolute"
         }
      }
   ]
}